{
  "name": "cube-vue3-modal",
  "version": "1.0.0-beta.3",
  "type": "module",
  "description": "A simple, basic, highly customizable and mobile friendly modal component for Vue3.",
  "author": "Fred Gomez <fred.gomez@me.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-inc/cube-vue3-modal.git"
  },
  "keywords": [
    "vue3",
    "modal"
  ],
  "files": [
    "dist",
    "src/assets"
  ],
  "main": "./dist/cube-vue3-modal.umd.cjs",
  "module": "./dist/cube-vue3-modal.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.0.11"
  }
